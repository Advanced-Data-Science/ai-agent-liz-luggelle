{
  "logging": {
    "level": "INFO",
    "log_file": "../logs/collection.log"
  },
  "api": {
    "use_api": true,
    "api_key_env_var": "WEATHER_API_KEY",
    "api_url": "http://api.openweathermap.org/data/2.5/weather",
    "cities": [
      "Portland,ME,US",
      "Boston,MA,US",
      "Albany,NY,US",
      "Burlington,VT,US",
      "New York,NY,US"
    ]
  },
  "collection": {
    "base_delay": 2.0,
    "max_requests": 5
  }
}
